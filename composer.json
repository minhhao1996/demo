{
    "name": "minhhao/laravel-minhhao",
    "description": "A middleware to protect your work in progress from prying eyes",
    "keywords": [
        "minhhao",
        "laravel-demo-minhhao"
    ],
    "homepage": "https://github.com/minhhao1996/demo",
    "license": "MIT",
    "authors": [
        {
            "name": "Minh Hao",
            "email": "haole042@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "MinhHao\\Demo": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MinhHao\\Demo\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MinhHao\\Demo\\DemoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}